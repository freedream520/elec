<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.jiangxi.elec.domain.ElecCommonMsg" table="Elec_CommonMsg">
		<id name="comID" type="string">
			<column name="ComID" not-null="true"></column>
			<generator class="uuid"/>
		</id>
		<property name="stationRun" type="string">
			<column name="StationRun" ></column>
		</property>
		<property name="createDate" type="date">
			<column name="CreateDate" length="50"></column>
		</property>
		<property name="devRun" type="string">
			<column name="DevRun"></column>
		</property>
	</class>
</hibernate-mapping>
