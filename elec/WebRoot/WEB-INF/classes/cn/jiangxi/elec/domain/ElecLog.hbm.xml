<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.jiangxi.elec.domain.ElecLog" table="Elec_Log">
		<id name="logID" type="string">
			<column name="logID" not-null="true"></column>
			<generator class="uuid"/>
		</id>
		<property name="ipAddress" type="string">
			<column name="ipAddress" ></column>
		</property>
		<property name="opeName" type="string">
			<column name="opeName" length="50"></column>
		</property>
		<property name="opeTime" type="timestamp">
			<column name="opeTime" length="50"></column>
		</property>
		<property name="details" type="string">
			<column name="details"></column>
		</property>
	</class>
</hibernate-mapping>
